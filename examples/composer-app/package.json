{
  "name": "composer",
  "version": "1.0.0",
  "description": "music application",
  "main": "public/index.html",
  "scripts": {
    "start": "yarn install && yarn build && yarn dev",
    "build": "yarn build:elm-spa && yarn build:elm",
    "build:elm-spa": "elm-spa build .",
    "build:elm": "yarn parcel build index.html",
    "dev": "concurrently --raw --kill-others \"yarn dev:elm-spa\" \"yarn dev:elm\"",
    "dev:elm-spa": "chokidar src/Pages -c \"yarn build:elm-spa\"",
    "dev:elm": "yarn parcel index.html"
  },
  "keywords": [
    "elm",
    "spa"
  ],
  "author": "Duncan Malashock",
  "license": "ISC",
  "devDependencies": {
    "chokidar-cli": "2.1.0",
    "concurrently": "5.0.0",
    "elm": "0.19.1-3",
    "elm-live": "4.0.2",
    "elm-spa": "4.1.0",
    "node-elm-compiler": "^5.0.4"
  },
  "dependencies": {
    "parcel-bundler": "^1.12.4",
    "webaudiofont": "^2.5.49",
    "webmidi": "^2.5.1"
  }
}
